#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,cs=0;
    cin>>t;
    while(t--)
    {
        cs++;
        int l,w,h;
        cin>>l>>w>>h;
        if((l<=20) && (w<=20) && (h<=20))
           printf("Case %d: good\n",cs);
        else
           printf("Case %d: bad\n",cs);
    }
    return 0;
}
