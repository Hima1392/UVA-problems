#include<stdio.h>
int main()
{
    int a,b,t;
    scanf("%d",&t);
    int cnt=0;
    while(t--)
    {
        cnt++;
        scanf("%d %d",&a,&b);
        int sum=0;
        for(int i=a;i<=b;i++)
        {
            if(i%2!=0)
                sum=sum+i;
        }
        printf("Case %d: %d\n",cnt,sum);
    }
    return 0;
}
