#include<stdio.h>
int main()
{
    int m,n,x,i,j,count,flag;
    while(scanf("%d%d",&m,&n)==2)
    {
        int count=0;
        for(int i=0;i<m;i++)
        {
            int flag=0;
            for(j=0;j<n;j++)
            {
                scanf("%d",&x);
                if(x==0)flag=1;
            }
            if(flag==0)count++;
        }
        printf("%d\n",count);
    }
    return 0;

}
